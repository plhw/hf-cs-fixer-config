{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base",
    ":preserveSemverRanges"
  ],
  "rebaseWhen": "conflicted",
  "hostRules": [
    {
      "hostType": "git-tags",
      "matchHost": "https://github.com/plhw",
      "encrypted": {
        "token": ""
      }
    },
    {
      "hostType": "docker",
      "matchHost": "https://ghcr.io/plhw",
      "encrypted": {
        "token": ""
      }
    }
  ]
}
